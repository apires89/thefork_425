<h1>Restaurants</h1>

<ul>
  <% @restaurants.each do |restaurant| %>
  <li>
    <%= link_to restaurant.name, restaurant_path(restaurant) %>
    <% restaurant.rating.times do  %>
    <i class="fas fa-star"></i>
    <% end %>
    <%= restaurant.address %>
  </li>
  <% end %>
</ul>

<%= link_to "New Restaurant", new_restaurant_path %>
